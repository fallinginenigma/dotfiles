[submodule "dotbot"]
	path = dotbot
	url = https://github.com/anishathalye/dotbot
	ignore = dirty
[submodule "vim/bundle/ctrlp.vim"]
	path = vim/pack/vendor/start/ctrlp.vim
	url = https://github.com/ctrlpvim/ctrlp.vim
	ignore = untracked
[submodule "vim/bundle/ultisnips"]
	path = vim/plugged/ultisnips
	url = https://github.com/SirVer/ultisnips
	ignore = untracked
[submodule "vim/bundle/vim-snippets"]
	path = vim/plugged/vim-snippets
	url = https://github.com/honza/vim-snippets
	ignore = untracked
[submodule "vim/bundle/vimtex"]
	path = vim/plugged/vimtex
	url = https://github.com/lervag/vimtex
	ignore = untracked
[submodule "zsh/plugins/dircolors-solarized"]
	path = shell/plugins/dircolors-solarized
	url = https://github.com/seebi/dircolors-solarized.git
[submodule "zsh/plugins/zsh-syntax-highlighting"]
	path = zsh/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting.git
[submodule "zsh/plugins/zsh-completions"]
	path = zsh/plugins/zsh-completions
	url = https://github.com/zsh-users/zsh-completions

